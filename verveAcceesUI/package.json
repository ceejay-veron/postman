{
  "name": "cypress_e2e_automation",
  "version": "1.0.0",
  "description": "cypress e2e automation, test automation",
  "main": "index.js",
  "bin": {
    "mochawesome-merge": "bin/mochawesome-merge.js"
  },
  "scripts": {
    "delete:reports": "(if exist cypress\\reports (rmdir /Q /S cypress\\reports)) && mkdir cypress\\reports && mkdir cypress\\reports\\mocha  &&  mkdir cypress\\reports\\mochareports",
    "pretest": "npm run delete:reports",
    "combine:reports": "jrm cypress/results/combined-report.xml \"cypress/reports/*.xml\"",
    "report": "cypress run --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json",
    "postreport": "npm run combine:reports",
    "test": "npm run posttest || npx cypress run || npm run posttest "
  },
  "keywords": [
    "cypress",
    "e2e",
    "automation"
  ],
  "author": "cipher veron",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^10.2.0",
    "cypress-multi-reporters": "^1.6.2",
    "faker": "^5.5.3",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3"
  }
}
